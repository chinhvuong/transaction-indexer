{
  "name": "nestjs-backend-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:evm-crawler": "node dist/modules/evm-crawler/main.js",
    "start:solana-crawler": "node dist/modules/solana-crawler/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "typeorm migration:generate -d dist/shared/configs/database.config.js src/database/migrations/$npm_config_name",
    "db:create": "typeorm migration:create",
    "db:run": "typeorm migration:run -d dist/shared/configs/database.config.js",
    "db:revert": "typeorm migration:revert -d dist/shared/configs/database.config.js",
    "db:show": "typeorm migration:show -d dist/shared/configs/database.config.js",
    "db:reset": "npm run db:revert && npm run db:run",
    "docker:start:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:restart:dev": "docker-compose -f docker-compose.dev.yml restart api",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build api",
    "docker:clean:dev": "docker-compose -f docker-compose.dev.yml down --volumes --remove-orphans && docker system prune -f",
    "docker:start:dev:clean": "npm run docker:clean:dev && npm run docker:start:dev",
    "docker:start:prod": "./scripts/deploy.sh",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs:prod": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:build": "docker build -t nestjs-backend-boilerplate .",
    "docker:setup-domain": "./scripts/setup-domain.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.1",
    "@fastify/multipart": "^9.0.3",
    "@fastify/static": "^8.2.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bull": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-fastify": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/bcrypt": "^6.0.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cloudinary": "^2.7.0",
    "ethers": "^6.15.0",
    "form-data": "^4.0.4",
    "handlebars": "^4.7.8",
    "ioredis": "^5.6.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typeorm": "^0.3.25",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
